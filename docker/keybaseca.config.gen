key_expiration: "+1h"
ssh_user: $SSH_USER
teams: [$SUBTEAMS]
keybase_paper_key: $PAPERKEY
keybase_username: $KEYBASE_USERNAME
use_subteam_as_principal: $USE_SUBTEAM_AS_PRINCIPAL
